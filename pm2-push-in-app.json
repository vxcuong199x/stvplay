[
  {
    "name": "push-in-app",
    "script": "./server/push-in-app.js",
    "error_file": "./log/push-in-app-err.log",
    "out_file": "./log/push-in-app-out.log",
    "pid_file": "./log/push-in-app.pid",
    "exec_mode": "fork_mode", //cluster_mode
    "instances" : "1",
    "watch": true,
    "ignore_watch": ["log", "node_modules"],
    "max_memory_restart" : "800M",
    "env": {
      "NODE_ENV": "production"
    }
  }
]
