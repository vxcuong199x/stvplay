{
  "name": "Feedback",
  "plural": "feedback",
  "base": "PersistedModel",
  "idInjection": true,
  "indexes": {
    "rankIndex": {
      "keys": {
        "day": 1,
        "username": 1
      }
    }
  },
  "mixins": {
    "CmsLog": {},
    "RemoteRouting": {
      "only": [
        "@find",
        "@create",
        "@findById",
        "@count",
        "@deleteById",
        "@replaceById",
        "updateAttributes",
        "@updateAll"
      ]
    },
    "ResultsetLimit": {
      "limit": 200
    },
    "TimeStamp": {
      "required": false,
      "validateUpsert": true
    }
  },
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "username": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "number",
      "required": true,
      "default": 1
    },
    "content": {
      "type": "string"
    },
    "status": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "vh",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
